name: "FPNN"
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  data_param {
    source: "/home/lmdb_64_train"
    batch_size: 32 
    backend: LMDB
  }
}

layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  data_param {
    source: "/home/lmdb_64_test"
    batch_size: 32
    backend: LMDB
  }
}

layer {
  name: "reshape_data"
  type: "Reshape"
  bottom: "data"
  top: "distance_field"
  reshape_param {
    shape {
      dim: 0 
      dim: 0 
      dim: 0 
      dim: 0 
      dim: 1
    }
  }
} 

layer {
  name: "gradient_field"
  type: "GradientField"
  bottom: "data"
  top: "normal_field" 
}

layer {
  name: "transform_3d"
  type: "Transform3D"
  bottom: "distance_field"
  bottom: "normal_field"
  bottom: "label"
  top: "distance_field_trans"
  top: "normal_field_trans"
  top: "label_trans"
  transform_3d_param {
    min_rotation_x: 0
    max_rotation_x: 0
    min_rotation_y: 0
    max_rotation_y: 0
    min_rotation_z: 0
    max_rotation_z: 360
    num_transformations: 4
    pad_value: 100
    pad_value: 0 
  }
}

layer {
  name: "reshape_distance"
  type: "Reshape"
  bottom: "distance_field_trans"
  top: "reshape_distance"
  reshape_param {
    shape {
      dim: 0 
      dim: 0 
      dim: 0 
      dim: -1
    }
  }
}

layer {
  name: "conv1_3d"
  type: "Convolution"
  bottom: "reshape_distance"
  top: "conv1_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }

  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    engine: CAFFE
    num_output: 128
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu1_3d"
  type: "PReLU"
  bottom: "conv1_3d"
  top: "conv1_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
}

layer {
  name: "conv2_3d"
  type: "Convolution"
  bottom: "conv1_3d"
  top: "conv2_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    engine: CAFFE
    num_output: 256
    kernel_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu2_3d"
  type: "PReLU"
  bottom: "conv2_3d"
  top: "conv2_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
}

layer {
  name: "conv3_3d"
  type: "Convolution"
  bottom: "conv2_3d"
  top: "conv3_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    engine: CAFFE
    num_output: 256
    kernel_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu3_3d"
  type: "PReLU"
  bottom: "conv3_3d"
  top: "conv3_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
}

layer {
  name: "conv4_3d"
  type: "Convolution"
  bottom: "conv3_3d"
  top: "conv4_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    engine: CAFFE
    num_output: 128
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu4_3d"
  type: "PReLU"
  bottom: "conv4_3d"
  top: "conv4_3d"
  param {
    lr_mult: 1
    decay_mult: 1
  }
}

layer {
  name: "fc0"
  type: "InnerProduct"
  bottom: "conv4_3d"
  top: "fc0"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "bn_fc0"
  type: "BatchNorm"
  bottom: "fc0"
  top: "fc0"
}

layer {
  name: "relu_fc0"
  type: "ReLU"
  bottom: "fc0"
  top: "fc0"
}

layer {
  name: "drop_fc0"
  type: "Dropout"
  bottom: "fc0"
  top: "fc0"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "softmax"
  type: "Softmax"
  bottom: "fc0"
  top: "attention_coeff"
}

layer {
  name: "concat_distance_normal_field"
  type: "Concat"
  bottom: "distance_field_trans"
  bottom: "normal_field_trans"
  top: "distance_normal_field"
  concat_param {
    axis: 4
  }
}

layer {
  name: "field_probing"
  type: "FieldProbing"
  bottom: "distance_normal_field"
  top: "distances_normals"
  param {
    lr_mult: 1
    decay_mult: 0
  }
  field_probing_param {
    num_sliding: 1
    padding: 0
    num_filter: 1024
    len_filter: 8
    min_span: 0.1 
    max_span: 0.4 
  }
}

layer {
  name: "slice"
  type: "Slice"
  bottom: "distances_normals"
  top: "distances"
  top: "normals"
  slice_param {
    axis: 6
    slice_point: 1
  }
}

layer {
  name: "gaussian"
  type: "Gaussian"
  bottom: "distances"
  top: "distances"
  gaussian_param {
    sigma: 8
  }
}

layer {
  name: "concat_distances_normals"
  type: "Concat"
  bottom: "distances"
  bottom: "normals"
  top: "concat_distances_normals" 
  concat_param {
    axis: 6
  }
}

layer {
  name: "dp"
  type: "DotProduct"
  bottom: "concat_distances_normals"
  top: "dp_"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  dot_product_param {
    weight_filler {
      type: "gaussian"
      std: 0.05
    }   
    bias_filler {
      type: "constant"
      value: 1
    }   
    share_weights: false
  }
}

layer {
  name: "reshape_dp"
  type: "Reshape"
  bottom: "dp_"
  top: "dp"
  reshape_param {
    shape {
      dim: 0
      dim: 0
      dim: 0
      dim: -1
    }
  }
}

layer {
  name: "bn_dp"
  type: "BatchNorm"
  bottom: "dp"
  top: "dp"
}

layer {
  name: "relu_dp"
  type: "ReLU"
  bottom: "dp"
  top: "dp"
}

layer {
  name: "fc1"
  type: "InnerProduct"
  bottom: "dp"
  top: "fc1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "gaussian"
      std: 0.05
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}

layer {
  name: "bn_fc1"
  type: "BatchNorm"
  bottom: "fc1"
  top: "fc1"
}

layer {
  name: "relu_fc1"
  type: "ReLU"
  bottom: "fc1"
  top: "fc1"
}

layer {
  name: "drop_fc1"
  type: "Dropout"
  bottom: "fc1"
  top: "fc1"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "attention"
  type: "Eltwise"
  bottom: "attention_coeff"
  bottom: "fc1"
  top: "attention"
  eltwise_param {
    operation: PROD
  }
}

layer {
  name: "fc2"
  type: "InnerProduct"
  bottom: "attention"
  top: "fc2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "gaussian"
      std: 0.05
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}

layer {
  name: "bn_fc2"
  type: "BatchNorm"
  bottom: "fc2"
  top: "fc2"
}

layer {
  name: "relu_fc2"
  type: "ReLU"
  bottom: "fc2"
  top: "fc2"
}

layer {
  name: "drop_fc2"
  type: "Dropout"
  bottom: "fc2"
  top: "fc2"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "fc3"
  type: "InnerProduct"
  bottom: "fc2"
  top: "fc3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "gaussian"
      std: 0.05
    }
    bias_filler {
      type: "constant"
      value: 1
    }
  }
}

layer {
  name: "bn_fc3"
  type: "BatchNorm"
  bottom: "fc3"
  top: "fc3"
}

layer {
  name: "relu_fc3"
  type: "ReLU"
  bottom: "fc3"
  top: "fc3"
}

layer {
  name: "drop_fc3"
  type: "Dropout"
  bottom: "fc3"
  top: "fc3"
  dropout_param {
    dropout_ratio: 0.5
  }
}

